{
  "name": "api",
  "version": "0.0.0",
  "description": "The TypeScript definition registry",
  "main": "dist/app.js",
  "files": [
    "dist/",
    "typings.json",
    "LICENSE"
  ],
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "build": "rm -rf dist/ && tsc",
    "build-watch": "tsc --watch",
    "test-spec": "ts-node node_modules/blue-tape/bin/blue-tape.js \"src/**/*.spec.ts\" | tap-spec",
    "test-cov": "ts-node node_modules/istanbul/lib/cli.js cover -e .ts --print none -x \"*.d.ts\" -x \"*.spec.ts\" blue-tape -- \"src/**/*.spec.ts\" | tap-spec",
    "test": "npm run lint && echo \"TODO: Write unit tests\"",
    "prepublish": "typings install && npm run build",
    "start": "nf start --wrap",
    "start-watch": "nodemon --exec \"npm run start\" --watch dist",
    "migrate": "migrate -c dist"
  },
  "engines": {
    "node": "4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/typings/typings.git"
  },
  "keywords": [
    "typings",
    "registry",
    "api",
    "json",
    "rest",
    "http"
  ],
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/typings/api/issues"
  },
  "homepage": "https://github.com/typings/api",
  "devDependencies": {
    "blue-tape": "^0.1.10",
    "foreman": "^1.4.1",
    "istanbul": "1.0.0-alpha.2",
    "nock": "^3.0.0",
    "nodemon": "^1.8.1",
    "pre-commit": "^1.0.6",
    "tap-spec": "^4.1.1",
    "ts-node": "^0.5.0",
    "tslint": "^3.1.1",
    "typescript": "^1.7.3",
    "typings": "^0.3.1"
  },
  "dependencies": {
    "basic-auth-connect": "^1.0.0",
    "batch": "^0.5.3",
    "express": "^4.13.3",
    "glob": "^6.0.1",
    "invariant": "^2.2.0",
    "knex": "^0.9.0",
    "kue": "^0.10.4",
    "migrate": "^0.2.2",
    "native-or-bluebird": "^1.2.0",
    "pg": "^4.4.3",
    "promise-finally": "^2.0.1",
    "semver": "^5.1.0",
    "thenify": "^3.1.1"
  }
}
